public class PrintSkipping {
    public static void main(String[] args) {
        int m = 1; // Starting number
        int n = 20; // Ending number
        int k = 3;  // Skip every k numbers

        for (int i = m; i <= n; i++) {
            System.out.print(i + " ");
            i += k - 1; // Skip the next k numbers
        }
    }
}
