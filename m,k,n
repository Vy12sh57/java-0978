
import java.util.Scanner;

public class MultiplicationTable {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Input the number for which the multiplication table is to be generated
        System.out.print("Enter the number (m): ");
        int m = scanner.nextInt();

        // Input the limit up to which the multiplication table should be printed
        System.out.print("Enter the limit (n): ");
        int n = scanner.nextInt();

        // Generate and print the multiplication table
        for (int i = 1; i <= n; i++) {
            System.out.println(m + " x " + i + " = " + (m * i));
        }

        scanner.close();
    }
}
